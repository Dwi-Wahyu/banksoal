                    </div>
                    <div class="ganti-data">
                        <h1>Ganti Data Profil Anda</h1>
                        <form action="" class="form-ganti">
                            <div class="row mt-1">
                                <div class="col-sm-6">
                                    <label
                                        for="inputText"
                                        class="col-form-label mb-1"
                                        >Nama :</label
                                    >
                                    <div>
                                        <input type="text" />
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label
                                        for="inputText"
                                        class="col-form-label mb-1"
                                        >Email :</label
                                    >
                                    <div>
                                        <input type="text" />
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label
                                        for="inputText"
                                        class="col-form-label mb-1"
                                        >Kata Kunci Lama :</label
                                    >
                                    <div>
                                        <input type="text" />
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <label
                                        for="inputText"
                                        class="col-form-label mb-1"
                                        >Kata Kunci Baru :</label
                                    >
                                    <div>
                                        <input type="text" />
                                    </div>
                                </div>
                            </div>

                            <div class="mt-3 repeat-pass">
                                <div class="repeat-label">
                                    <label for="inputText"
                                        >Kata Kunci Baru (Konfirmasi) :</label
                                    >
                                </div>
                                <div class="repeat-input">
                                    <input type="text" id="repeat-input" />
                                </div>
                            </div>
                            <div
                                class="row-cols-6 mt-3 d-flex justify-content-center"
                            >
                                <button
                                    class="btn btn-primary"
                                    style="margin-right: 1rem; width: 6rem"
                                >
                                    Simpan
                                </button>

                                <button
                                    type="button"
                                    class="btn btn-primary"
                                    data-bs-toggle="modal"
                                    data-bs-target="#ganti-gambar"
                                >
                                    Ganti Foto Profil
                                </button>
                            </div>

                            <!-- Vertically centered Modal -->
                            <div
                                class="modal fade"
                                id="ganti-gambar"
                                tabindex="-1"
                            >
                                <div class="modal-dialog modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5
                                                class="modal-title"
                                                style="font-weight: 600"
                                            >
                                                Ganti Foto
                                            </h5>
                                            <button
                                                type="button"
                                                class="btn-close"
                                                data-bs-dismiss="modal"
                                                aria-label="Close"
                                            ></button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="gambar">
                                                <div class="input-gambar">
                                                    <input
                                                        type="file"
                                                        name="gambar"
                                                        id="gambar"
                                                        class="input-container mb-2"
                                                        title="Tidak ada berkas"
                                                    />
                                                    <p>
                                                        <strong>
                                                            Perhatian:
                                                        </strong>
                                                        Gambar Tidak boleh lebih
                                                        dari 512 KB atau 0.5 MB,
                                                        ukuran dimensi gambar
                                                        maksimal lebar 128 pixel
                                                        dan tinggi 128 pixel.
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button
                                                type="button"
                                                class="btn btn-primary"
                                            >
                                                Ganti Foto
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- End Vertically centered Modal-->
